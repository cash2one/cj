
<!-- 提醒关注弹出框 -->
<div id="modal-invite-member" class="modal modal-alert modal-info fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <i class="fa fa-info-circle"></i>
            </div>
            <div class="modal-title">提醒要关注</div>
            <div class="modal-body">
                <span class=" text-info span_invite_member"></span>
                <p class="text-default"><span class="span_invite_count"></span>位同事关注吗？</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn" data-dismiss="modal">取消</button>
                <button type="button" id="btn_invite_member" class="btn btn-info" data-dismiss="modal">确定</button>
            </div>
        </div>
    </div>
</div>
<!-- 提醒关注弹出框 -->


<!-- 删除用户弹出框 -->
<div id="modal-delete-member" class="modal modal-alert modal-danger fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <i class="fa fa-times-circle"></i>
            </div>
            <div class="modal-title">确定要删除</div>
            <div class="modal-body">
                <span class=" text-info span_member"></span>
                <p class="text-default"><span class="span_member_count"></span>位同事吗？</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn" data-dismiss="modal">取消</button>
                <button type="button" id="btn_delete_member" class="btn btn-danger" data-dismiss="modal">确定</button>
            </div>
        </div>
    </div>
</div>
<!-- 删除用户弹出框 -->